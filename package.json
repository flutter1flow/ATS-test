{
	"name": "ats-test",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"deploy": "node scripts/register-webhook.js & wrangler deploy",
		"dev": "wrangler types & wrangler dev",
		"test": "vitest run --coverage --passWithNoTests",
		"cf-typegen": "wrangler types",
		"postbuild": "wrangler types",
		"build": "tsc",
		"lint": "eslint src --cache --fix",
		"prepare": "husky install",
		"prettier:check": "prettier --check 'src/**/*.{js,ts,json,css,md}'",
		"prettier:fix": "prettier --write 'src/**/*.{js,ts,json,css,md}'",
		"test:cli": "node test.cli.js",
		"tsc": "npx tsc --noEmit",
		"test-local": "echo The command for testing the bot locally was changed to: 'npm run test:cli'"
