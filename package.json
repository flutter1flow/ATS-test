		"@vitest/coverage-istanbul": "^2.1.8",
		"@vitest/coverage-v8": "^2.1.8",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"globals": "^15.14.0",
		"husky": "^8.0.0",
		"lint-staged": "^15.2.11",
		"prettier": "^3.4.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.2",
		"typescript-eslint": "^8.18.1",
		"vitest": "^2.1.8",
		"wrangler": "^3.60.3"
	},
	"dependencies": {
		"dotenv": "^16.4.7",
		"node-fetch": "^3.3.2",
		"reflect-metadata": "^0.2.2",
		"tsyringe": "^4.8.0",
		"winston": "^3.17.0"
	}
